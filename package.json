{
  "name": "social-crawl-monorepo",
  "version": "1.0.0",
  "description": "Social crawl monorepo with multiple services",
  "private": true,
  "scripts": {
    "build": "pnpm run -r build",
    "build:infrastructure": "pnpm --filter @social-crawl/infrastructure build",
    "build:main": "pnpm --filter main-crawler build",
    "build:venue": "pnpm --filter venue-enrichment build",
    "dev:main": "pnpm --filter main-crawler dev",
    "dev:venue": "pnpm --filter venue-enrichment dev",
    "clean": "pnpm run -r clean",
    "build:local": "./scripts/build.sh local",
    "build:prod": "./scripts/build.sh prod",
    "deploy:check": "./scripts/deploy.sh prod check",
    "deploy:setup": "./scripts/deploy.sh prod setup",
    "deploy:prod": "./scripts/deploy.sh prod deploy",
    "deploy:remove": "./scripts/deploy.sh prod remove",
    "setup:secrets": "./scripts/setup-secrets.sh",
    "setup:queues": "./scripts/setup-queues.sh",
    "setup:local": "./scripts/setup-local.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/turndown": "^5.0.5",
    "nitro": "^0.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.862.0",
    "@azure/service-bus": "^7.9.5",
    "@azure/storage-blob": "^12.28.0",
    "@google/genai": "^1.12.0",
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.1",
    "fluent-ffmpeg": "^2.1.3",
    "fuzzball": "^2.2.3",
    "h3": "^1.15.3",
    "https-proxy-agent": "^7.0.6",
    "mongodb": "^6.18.0",
    "nitropack": "^2.12.4",
    "ofetch": "^1.4.1",
    "playwright": "^1.54.1",
    "sharp": "^0.34.3",
    "string-similarity": "^4.0.4",
    "turndown": "^7.2.1",
    "zenrows": "^2.0.1"
  }
}
